<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.kurisinis.fxControllers.MainForm">
   <children>
      <TabPane fx:id="tabsPane" layoutY="-7.0" prefHeight="513.0" prefWidth="721.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="userTab" onSelectionChanged="#reloadTableData" text="User management">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView layoutX="14.0" layoutY="14.0" prefHeight="354.0" prefWidth="693.0">
                          <columns>
                            <TableColumn fx:id="IdColl" prefWidth="75.0" text="IdCa" />
                            <TableColumn fx:id="LoginColl" prefWidth="75.0" text="Login" />
                              <TableColumn fx:id="PasswordCall" prefWidth="75.0" text="Password" />
                              <TableColumn fx:id="NameColl" prefWidth="75.0" text="Name" />
                              <TableColumn fx:id="SurnameColl" prefWidth="75.0" text="Surname" />
                              <TableColumn fx:id="DummyColl" prefWidth="75.0" text="Action" />
                              <TableColumn fx:id="OrderHistoryColl" prefWidth="75.0" text="Order history" />
                          </columns>
                        </TableView>
                        <Button layoutX="309.0" layoutY="420.0" mnemonicParsing="false" text="Create new User" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
            <Tab fx:id="managementTab" onSelectionChanged="#reloadTableData" text="Orders managment">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Button layoutX="309.0" layoutY="420.0" mnemonicParsing="false" text="Create new User" />
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
            <Tab fx:id="foodTab" onSelectionChanged="#reloadTableData" text="Food management">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Button layoutX="309.0" layoutY="420.0" mnemonicParsing="false" text="Create new User" />
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
            <Tab fx:id="altTab" onSelectionChanged="#reloadTableData" text="Alternative user management">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Button layoutX="104.0" layoutY="298.0" mnemonicParsing="false" onAction="#addUser" text="Create new User" />
                        <ListView fx:id="userListField" layoutX="17.0" layoutY="42.0" prefHeight="228.0" prefWidth="687.0" />
                        <Button layoutX="286.0" layoutY="298.0" mnemonicParsing="false" onAction="#loadUser" text="Update user" />
                        <Button layoutX="475.0" layoutY="298.0" mnemonicParsing="false" onAction="#deleteUser" text="Delete user" />
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
